@model DarboOrganizavimoPlatforma.Domains.Team

@{
    ViewData["Title"] = "Details";
}

<h1>@Model.TeamName Details</h1>

<div>
    <h4>Team</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TeamName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TeamName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TeamDescription)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TeamDescription)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CreateTime)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CreateTime)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Company.CompanyName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Company.CompanyName)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-success" asp-action="EditTeam" asp-route-id="@Model.TeamId">Edit</a> |
    <a class="btn btn-primary" asp-action="Index">Back to Team List</a>
</div>
